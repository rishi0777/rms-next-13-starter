{
  "name": "smdb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start source .env",
    "dev:https": "concurrently \"pnpm run ssl-proxy\" \"pnpm run dev\"",
    "ssl-proxy": "source .env && local-ssl-proxy --key ${SSL_KEY_FILE} --cert ${SSL_CERT_FILE} --source 6007 --target 6006",
    "prepare": "is-ci || husky install",
    "format": "prettier . --write",
    "lint:js": "eslint . --fix --cache",
    "lint:css": "stylelint \"**/*.scss\"",
    "type-check": "tsc",
    "check-all": "pnpm run type-check && pnpm run lint:js && pnpm run lint:css"
  },
  "dependencies": {
