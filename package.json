    "@types/nodemailer": "^6.4.8",
    "@types/prettier": "^2.7.3",
    "@types/react": "^17.0.62",
    "@types/react-dom": "^17.0.20",
    "@types/ua-parser-js": "^0.7.36",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
