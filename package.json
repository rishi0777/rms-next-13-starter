    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "classnames": "^2.3.2",
    "eslint": "^8.44.0",
    "eslint-config-next": "^13.4.7",
    "next": "^13.4.7",
    "next-build-id": "^3.0.0",
    "next-compose-plugins": "^2.2.1",
    "next-transpile-modules": "^10.0.0",
